CONSTRUCTORS

car : engine * type -> car
g : type -> type

electric : engine
diesel : engine
hybrid : engine
gas : engine

family : type
van : type
suv : type

red : color
blue : color
white : color

FUNCTIONS

paint : car -> color

RULES

paint(car(diesel(),g(family()))) -> red()
paint(car(x,g(suv()))) -> red()
paint(car(diesel(),g(van()))) -> red()

